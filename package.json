{
  "name": "nexal",
  "version": "1.0.41",
  "description": "Ein vollst√§ndiges Next.js Starter-Template",
  "main": "index.js",
  "scripts": {
    "dev": "npm run prisma:prepare && next dev",
    "prisma:prepare": "prisma generate --schema src/prisma/schema.prisma && prisma db push --schema src/prisma/schema.prisma",
    "build": "prisma generate --schema src/prisma/schema.prisma && next build",
    "start": "next start",
    "lint": "eslint",
    "prisma:generate": "prisma generate --schema src/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema src/prisma/schema.prisma",
    "init:admin": "node scripts/init-admin.js",
    "setup:db": "node scripts/setup-database.js",
    "validate:db": "node scripts/validate-db-config.js",
    "check:prisma": "node -e \"const p=require('./package.json'); const a=(p.dependencies&&p.dependencies['@prisma/client'])||''; const b=(p.devDependencies&&p.devDependencies.prisma)||''; if(a!==b){console.warn('WARN: prisma and @prisma/client versions differ:', b, a); process.exit(1);} else {console.log('OK: Prisma versions match:', a);}\"",
    "postinstall": "npm run check:prisma"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "keywords": [
    "nextjs",
    "template",
    "starter",
    "react",
    "typescript",
    "tailwind"
  ],
  "author": "Jonasppxx",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jonasppxx/nexal.git"
  },
  "files": [
    "src",
    "public",
    "bin",
    "index.js",
    "*.config.*",
    "*.json",
    ".gitignore",
    ".gitattributes",
    "README.md",
    ".env.example"
  ],
  "bin": {
    "create-nexal": "./index.js",
    "nexal": "./index.js"
  },
  "dependencies": {
    "@prisma/client": "6.19.0",
    "bcryptjs": "^2.4.3",
    "better-auth": "^1.3.34",
    "dotenv": "^17.2.3",
    "next": "16.0.1",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "prisma": "6.19.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
